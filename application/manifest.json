{
  "manifest_version": 2,
  "version": "0.1",
  "name": "FileExtension",
  "description": "This extention shows information about users files and folders.",
  //"key": "",

  "browser_action": 
  {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "content_scripts": [{
    "js": ["500px.js", "popup.js"],
    "matches": ["http://*/*", "https://*/*"]
  }],
  "content_security_policy": "script-src 'self' https://api.500px.com; object-src 'self'",


  "permissions": 
  [
    "identity",
    "identity.email",
    "nativeMessaging",
    "tabs"
  ],

  "background": {
      "scripts": ["background.js", "500px.js"]
  },
    "oauth2": {
      "client_id": "81345675471-c20rgohcsd4nqsmkpu1562qnusjvp0u3.apps.googleusercontent.com",
      "scopes": ["profile"]
    }
}